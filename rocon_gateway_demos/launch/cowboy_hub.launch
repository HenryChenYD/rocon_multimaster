<!-- 
  Launches the gateway hub (redis data store + zeroconf advertisement)
-->
<launch>
  <node pkg="rocon_gateway_hub" type="hub.py" name="gateway_hub" output="screen">
    <rosparam command="load" file="$(find rocon_gateway_hub)/param/default.yaml" />
    <rosparam param="name">Cowboy Hub</rosparam>
    <rosparam param="zeroconf">false</rosparam>
  </node>
</launch>
